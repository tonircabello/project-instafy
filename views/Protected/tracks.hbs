
  {{>navbar}}
<h2>Tracks</h2>
<ul>
  {{#each tracks}}
    <li>
      <h3>{{this.name}}</h3>
      {{#if this.preview_url}}
        <audio controls>
          <source src="{{this.preview_url}}" type="audio/mpeg">
          Your browser does not support the audio element.
        </audio>
      {{else}}
        <p>No preview available</p>
      {{/if}}
    </li>
  {{/each}}
</ul>
